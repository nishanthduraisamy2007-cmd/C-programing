# Input list from the image
numbers = [1, 9, 10, -10, 15]

# Initialize counters
single_digit = 0
double_digit = 0
mult_3 = 0
mult_5 = 0
positive = 0
negative = 0

for n in numbers:
    # Use absolute value to correctly identify digit counts for negative numbers
    abs_n = abs(n)
    
    # 1. Count Single-digit and Double-digit
    if 0 <= abs_n <= 9:
        single_digit += 1
    elif 10 <= abs_n <= 99:
        double_digit += 1
        
    # 2. Count Multiples of 3 and 5
    if n % 3 == 0:
        mult_3 += 1
    if n % 5 == 0:
        mult_5 += 1
        
    # 3. Count Positive and Negative numbers
    if n > 0:
        positive += 1
    elif n < 0:
        negative += 1

# Output the results
print(single_digit)
print(double_digit)
print(mult_3)
print(mult_5)
print(positive)
print(negative)
